
<!DOCTYPE html>
<html lang="fr">
<head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title> {% block title %}Fondation Samuel Williams{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('frontend/images/favicon.ico')}}">
        <!--STYLESHEET-->
        <!--=================================================-->
        {% block stylesheets %}
        <!--Roboto Slab Font [ OPTIONAL ] -->
        <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:300,400,700|Roboto:300,400,700" rel="stylesheet">
        <link href="{{ asset('backoffice/css/bootstrap.min.css')}}" rel="stylesheet">
        <link href="{{ asset('backoffice/css/style.css')}}" rel="stylesheet">
        <link href="{{ asset('backoffice/plugins/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet">
        <link href="{{ asset('backoffice/plugins/switchery/switchery.min.css')}}" rel="stylesheet">
        <link href="{{ asset('backoffice/plugins/bootstrap-select/bootstrap-select.min.css')}}" rel="stylesheet">

        <!--SCRIPT-->
        <!--=================================================-->
        <!--Page Load Progress Bar [ OPTIONAL ]-->
        <link href="{{ asset('backoffice/plugins/pace/pace.min.css')}}" rel="stylesheet">
        <script src="{{ asset('backoffice/plugins/pace/pace.min.js')}}"></script>
        <style media="screen">
          .img-page{ padding-right: 10px; margin-right: 15px; float: left;}
        </style>
        {% endblock %}


    </head>
    <body>
        <div id="container" class="effect mainnav-lg navbar-fixed mainnav-fixed">
            <!--NAVBAR-->
            <!--===================================================-->
            <header id="navbar">
                <div id="navbar-container" class="boxed">
                    <!--Brand logo & name-->
                    <!--================================-->
                    <div class="navbar-header">
                        <a href="#" class="navbar-brand">
                            <!--<img src="backoffice/img/logo.png" width="25">-->
                            <div class="brand-title">
                                <span class="brand-text">FSW</span>
                            </div>
                        </a>
                    </div>
                    <!--================================-->
                    <!--End brand logo & name-->
                    <!--Navbar Dropdown-->
                    <!--================================-->
                    <div class="navbar-content clearfix">
                        <ul class="nav navbar-top-links pull-left">
                            <!--Navigation toogle button-->
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <li class="tgl-menu-btn">
                                <a class="mainnav-toggle" href="#"> <i class="fa fa-navicon fa-lg"></i> </a>
                            </li>
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <!--End Navigation toogle button-->
                            <!--Profile toogle button-->
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <!--End notifications dropdown-->
                        </ul>
                        <ul class="nav navbar-top-links pull-right">
                            <!--Fullscreen toogle button-->
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <li class="hidden-xs" id="toggleFullscreen">
                                <a class="fa fa-expand" data-toggle="fullscreen" href="#" role="button">
                                <span class="sr-only">Toggle fullscreen</span>
                                </a>
                            </li>
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <!--End Fullscreen toogle button-->
                            <!--User dropdown-->
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <li id="dropdown-user" class="dropdown">
                                <a href="#" data-toggle="dropdown" class="dropdown-toggle text-right">
                                    <span class="pull-right"> <img class="backoffice/img-circle img-user media-object" src="{{ asset('backoffice/img/avatar.png')}}" alt="Profile Picture"> </span>
                                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                          					    <div class="username hidden-xs">
                                        {{ 'layout.user'|trans({'%username%': app.user.username}, 'FOSUserBundle')|title }}
                                        </div>
                          					{% else %}
                          					    <div class="username hidden-xs">
                          								<a href="{{ path('fos_user_security_login') }}" title="Connecte-toi" style="color: white;">Anonyme</a>
                          							</div>
                          					{% endif %}
                                </a>
                                <div class="dropdown-menu dropdown-menu-right with-arrow">
                                    <!-- User dropdown menu -->
                                    <ul class="head-list">
                                        <li>
                                            <a href="#"> <i class="fa fa-cog fa-fw"></i> Changer mot de passe </a>
                                        </li>
                                        <li>
                                            <a href="#"> <i class="fa fa-sign-out fa-fw"></i> Deconnexion </a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                            <!--End user dropdown-->
                            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                        </ul>
                    </div>
                    <!--================================-->
                    <!--End Navbar Dropdown-->
                </div>
            </header>
            <!--===================================================-->
            <!--END NAVBAR-->
            <div class="boxed">
                <!--CONTENT CONTAINER-->
                <!--===================================================-->
                <div id="content-container">
                  <!--Page Title-->

                  {% block body %}{% endblock %}
                </div>
                <!--===================================================-->
                <!--END CONTENT CONTAINER-->
                <!--MAIN NAVIGATION-->
                <!--===================================================-->
                 <nav id="mainnav-container">
                    <div id="mainnav">
                        <!--Menu-->
                        <!--================================-->
                        <div id="mainnav-menu-wrap">
                            <div class="nano">
                                <div class="nano-content">
                                    <ul id="mainnav-menu" class="list-group">
                                        <!--Category name-->
                                        <li class="list-header">Accueil</li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-flask"></i>
                                            <span class="menu-title">
                                               Tableau de bord
                                            </span>
                                            </a>
                                        </li>
                                        <!--Category name-->
                                        <li class="list-header">Site web</li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-globe"></i>
                                            <span class="menu-title">
                                               Qui sommes-nous?
                                            </span>
                                            <i class="arrow"></i>
                                            </a>
                                            <!--Submenu-->
                                            <ul class="collapse">
                                                <li><a href="#"><i class="fa fa-caret-right"></i> Présentation </a></li>
                                                <li><a href="#"><i class="fa fa-caret-right"></i> Equipe dirigéante </a></li>
                                                <li><a href="#"><i class="fa fa-caret-right"></i> Objectifs </a></li>
                                            </ul>
                                        </li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-globe"></i>
                                            <span class="menu-title">
                                               Interventions
                                            </span>
                                            <i class="arrow"></i>
                                            </a>
                                            <!--Submenu-->
                                            <ul class="collapse">
                                                <li><a href="{{ path('admin_axe_index')}}"><i class="fa fa-caret-right"></i> Axes </a></li>
                                                <li><a href="{{ path('admin_zone_index')}}"><i class="fa fa-caret-right"></i> Zone </a></li>
                                            </ul>
                                        </li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="{{ path('admin_action_index')}}">
                                            <i class="fa fa-globe"></i>
                                            <span class="menu-title">
                                               Actions
                                            </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-globe"></i>
                                            <span class="menu-title">
                                               Gallerie
                                            </span>
                                            </a>
                                        </li>
                                        <li class="list-header">Parrainage</li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="{{ path('admin_parrain_index')}}">
                                            <i class="fa fa-user"></i>
                                            <span class="menu-title">
                                               Parrains
                                            </span>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-users"></i>
                                            <span class="menu-title">
                                               Filleuls
                                            </span>
                                            </a>
                                        </li>
                                        <li class="list-divider"></li>
                                        <!--Category name-->
                                        <li class="list-header">Administrateur</li>
                                        <!--Menu list item-->
                                        <li>
                                            <a href="#">
                                            <i class="fa fa-lock"></i>
                                            <span class="menu-title">
                                            Sécurité
                                            </span>
                                            </a>
                                            <!--Submenu-->
                                            <ul class="collapse">
                                                <li><a href="#"><i class="fa fa-caret-right"></i>Utilisateurs</a></li>
                                                <li><a href="#"><i class="fa fa-caret-right"></i>Gestionnaires</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                    <!--Widget-->
                                    <!--================================-->
                                    <div class="mainnav-widget">
                                        <!-- Show the button on collapsed navigation -->
                                        <div class="show-small">
                                            <a href="#" data-toggle="menu-widget" data-target="#demo-wg-server">
                                            <i class="fa fa-desktop"></i>
                                            </a>
                                        </div>
                                        <!-- Hide the content on collapsed navigation -->
                                    </div>
                                    <!--================================-->
                                    <!--End widget-->
                                </div>
                            </div>
                        </div>
                        <!--================================-->
                        <!--End menu-->
                    </div>
                </nav>
                <!--===================================================-->
                <!--END MAIN NAVIGATION-->
                <!--ASIDE-->
                <!--===================================================-->
            </div>
            <!-- FOOTER -->
            <!--===================================================-->
            <footer id="footer">
                <!-- Visible when footer positions are static -->
                <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                <div class="hide-fixed pull-right pad-rgt">BackOffice<a href="http://cimetech-ci.com/" title="" style="font-size: 0.8em; padding-left: 25px;" target="_blank"> By CIME</a></div>
                <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                <!-- Remove the class name "show-fixed" and "hide-fixed" to make the content always appears. -->
                <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                <p class="pad-lft">&#0169; 2017
                    <span class="hidden-xs">Cabinet Ivoirien de Multi-services et d'Expertises</span>
                    <span class="hidden-sm hidden-md hidden-lg">CIME</span>
                </p>
            </footer>
            <!--===================================================-->
            <!-- END FOOTER -->
            <!-- SCROLL TOP BUTTON -->
            <!--===================================================-->
            <button id="scroll-top" class="btn"><i class="fa fa-chevron-up"></i></button>
            <!--===================================================-->
        </div>
        <!--===================================================-->
        <!-- END OF CONTAINER -->
        <!--JAVASCRIPT-->
        <!--=================================================-->
        <!--jQuery [ REQUIRED ]-->
        {% block javascripts %}
        <script src="{{ asset('backoffice/js/jquery-2.1.1.min.js')}}"></script>
        <script src="{{ asset('backoffice/js/bootstrap.min.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/fast-click/fastclick.min.js')}}"></script>
        <script src="{{ asset('backoffice/js/scripts.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/nanoscrollerjs/jquery.nanoscroller.min.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/metismenu/metismenu.min.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/switchery/switchery.min.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/bootstrap-select/bootstrap-select.min.js')}}"></script>
        <script src="{{ asset('backoffice/plugins/screenfull/screenfull.js')}}"></script>
        {% endblock %}

    </body>

</html>
